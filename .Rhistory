ggplot(data = peering2, aes(x = x, y = y, label = labs)) +
geom_point(
data = peering2,
size = ifelse(peering2$y == 5 & peering2$x == -.5 | peering2$y == 7 & peering2$x == .5, 5, 3),
alpha = ifelse(peering2$y == 5, 1, .75),
aes(x = x, y = y, colour = col)
) +
geom_text() +
